@{
    ViewData["Title"] = "Home Page";
}

@model IndexViewModel

<div class="container">
    <h2>
        @Model.Team
    </h2>

    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th>First Name</th>
                <th>Middle Initial</th>
                <th>Last Name</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
                <th>Phone Number</th>


            </tr>
        </thead>
        <tbody>
            @foreach (var x in Model.Bowler)
            {
                <tr>
                    <td>@x.BowlerFirstName</td>
                    <td>@x.BowlerMiddleInit</td>
                    <td>@x.BowlerLastName</td>
                    <td>@x.BowlerAddress</td>
                    <td>@x.BowlerCity</td>
                    <td>@x.BowlerState</td>
                    <td>@x.BowlerZip</td>
                    <td>@x.BowlerPhoneNumber</td>
                </tr>
            }

        </tbody>
    </table>
</div>

<div page-info="@Model.PageNumberingInfo" team="@Model.Team"
        page-url-team="@Model.Team">

</div>
